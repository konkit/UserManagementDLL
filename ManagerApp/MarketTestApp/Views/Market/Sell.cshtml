@model IEnumerable<MarketTestApp.Models.Item>

@{
    ViewBag.Title = "Sell";
}

<h2>Sell</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td> 
            @Html.ActionLink("Sell", "DoSell", new { id=item.Id })
        </td>
    </tr>
}

</table>
